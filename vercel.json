{
    "builds": [
        {
            "src": "truecaller/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" , "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "truecaller/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "truecaller.settings"
    }
}
